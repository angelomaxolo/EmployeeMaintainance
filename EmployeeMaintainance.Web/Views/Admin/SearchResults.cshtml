
@using EmployeeMaintainance.Web.ViewModels
@model IEnumerable<SearchEmployeeViewModel>
@{
    ViewBag.Title = "Search Results";
    Layout = "_Layout";
}

<h2>Employees</h2>
<hr />

<h4>Employee Numbers</h4>

@foreach (var employee in Model)
{
    <ul class="employees">
        <li>
            <div class="employeeNumber">
                <a href="@Url.Action("Employee", "Admin", new {id = employee.Id})">@employee.EmployeeNumber</a>
            </div>
        </li>
    </ul>
}
